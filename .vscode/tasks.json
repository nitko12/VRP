{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: g++-11 build active file",
      "command": "/usr/bin/g++",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}",
        "-std=c++17",
        "-Isrc/",
        "-I${GUROBI_HOME}/include/",
        "-L${GUROBI_HOME}/lib",
        "-lgurobi_c++",
        "-lgurobi91"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}
